{
  "name": "trendsoft/laravel-capital",
  "keywords": [
    "capital",
    "trendsoft",
    "laravel",
    "sdk",
    "amount"
  ],
  "description": "基于`trendsoft/capital`的Laravel金额转中文大写",
  "license": "MIT",
  "homepage": "http://www.trendsoft.org",
  "authors": [
    {
      "name": "jabber",
      "email": "2898117012@qq.com",
      "homepage": "http://jabber.oppophp.com",
      "role": "Developer"
    }
  ],
  "support": {
    "wiki": "https://github.com/trendsoft/laravel-capital/wiki",
    "issues": "https://github.com/trendsoft/laravel-capital/issues"
  },
  "require": {
    "php": ">=7.0",
    "illuminate/container": "^5.5",
    "trendsoft/capital": "^0.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "mockery/mockery": "^1.0",
    "phpunit/php-invoker": "^1.1",
    "graham-campbell/testbench": "^4.0",
    "graham-campbell/analyzer": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "LaravelCapital\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelCapital\\ServiceProvider"
      ],
      "aliases": {
        "Capital": "LaravelCapital\\Capital"
      }
    }
  }
}
